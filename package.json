{
  "name": "b64-cli",
  "version": "1.0.0",
  "description": "Command line interface to decode/encode base64 data",
  "main": "cli.js",
  "scripts": {
    "debug": "babel-node --inspect --debug-brk cli.js",
    "test": "xo && babel-node test.js | faucet"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/haroun/b64-cli.git"
  },
  "keywords": [
    "b64",
    "base64",
    "encode",
    "decode",
    "cli-app",
    "cli",
    "atob",
    "btoa"
  ],
  "author": "Harouna Traore",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/haroun/b64-cli/issues"
  },
  "homepage": "https://github.com/haroun/b64-cli#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "execa": "^0.8.0",
    "faucet": "0.0.1",
    "tape": "^4.8.0",
    "xo": "^0.18.2"
  },
  "dependencies": {
    "get-stdin": "^5.0.1",
    "meow": "^3.7.0"
  },
  "bin": {
    "b64": "cli.js"
  },
  "files": [
    "cli.js"
  ],
  "xo": {
    "space": true,
    "semicolon": false
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ]
  }
}
